 [class^="icon-"]:before
 [class*=" icon-"]:before
  font-family "fontello"
  font-style normal
  font-weight normal
  speak none
  display inline-block
  text-decoration none
  text-align center
  font-variant normal
  text-transform none
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-right:before
  content '\e801'
  
.icon-left:before
  content '\e800'
  
.icon-sound-off:before
  content '\e805'
  
.icon-sound-on:before
  content '\e806'
  
.icon-move:before
  content '\e802'
  
.icon-place:before
  content '\e804'
  
.icon-report:before
  content '\e803'